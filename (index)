<script>
  const segments = document.querySelectorAll('.wave-segment');
  let t = 0;

  function animateWave() {
    t += 0.08;
    segments.forEach((segment, i) => {
      const amplitude = 10;
      const frequency = 0.4;
      const y = Math.sin(t + i * frequency) * amplitude;
      segment.style.transform = `translateY(${y}px)`;
    });
    requestAnimationFrame(animateWave);
  }

  animateWave();
</script>

